{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8083",
  "dependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.19",
    "axios": "^1.6.2",
    "bulma": "^0.9.4",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "react-scripts": "5.0.1",
    "react-scroll-to-top": "^3.0.0",
    "react-toastify": "^9.1.3"
  },
  "scripts": {
    "start": "cross-env PORT=8082 HOST=127.0.0.1 react-scripts start",
    "dev": "cross-env PORT=8082 react-scripts start",
    "build": "npm run css-build && react-scripts build",
    "test": "cross-env PORT=8082 react-scripts test",
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss public/css/mystyles.css",
    "css-watch": "npm run css-build -- --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass": "^9.0.0"
  }
}
